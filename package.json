        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-pngquant": "^9.0.0",
        "intersection-observer": "^0.11.0",
        "imports-loader": "^1.1.0",
        "license-info-webpack-plugin": "^2.1.1",
        "lint-staged": "^10.2.11",
        "node-sass-package-importer": "^5.3.2",
        "object-fit-images": "^3.2.4",
        "picturefill": "^3.0.3",
